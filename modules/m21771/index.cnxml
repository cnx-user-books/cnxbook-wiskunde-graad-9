                  <entry>=</entry>
                  <entry>wis alles, insluitende die geheue, uit</entry>
                </row>
                <row>
                  <entry>C</entry>
                  <entry>=</entry>
                  <entry>maak die vertoonvenster skoon</entry>
                </row>
                <row>
                  <entry>CE / CM</entry>
                  <entry>=</entry>
                  <entry>maak die geheue skoon</entry>
                </row>
                <row>
