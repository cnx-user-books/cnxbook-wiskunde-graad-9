          <colspec colnum="8" colname="c8"/>
          <colspec colnum="9" colname="c9"/>
          <tbody>
            <row>
              <entry>Dae </entry>
              <entry>1</entry>
              <entry>2</entry>
              <entry>3</entry>
              <entry>4</entry>
              <entry/>
              <entry>12</entry>
              <entry>35</entry>
              <entry/>
            </row>
            <row>
              <entry>Kiste</entry>
              <entry>50</entry>
              <entry>100</entry>
              <entry/>
              <entry/>
              <entry>250</entry>
              <entry/>
