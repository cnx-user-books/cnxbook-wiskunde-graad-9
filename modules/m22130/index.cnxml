    <section id="id8659164">
      <title>MODULE TOETS </title>
      <para id="id8981937">1. <emphasis effect="bold">Verbind die korrekte antwoord in kolom B met die definisie in kolom A:</emphasis></para>
      <table id="id6235580" summary="">
        <tgroup cols="3">
          <colspec colnum="1" colname="c1"/>
          <colspec colnum="2" colname="c2"/>
          <colspec colnum="3" colname="c3"/>
          <tbody>
            <row>
              <entry>A</entry>
              <entry/>
              <entry>B</entry>
            </row>
            <row>
              <entry>Die kortste moontlike lyn</entry>
              <entry/>
              <entry>halflyn / straal</entry>
            </row>
            <row>
              <entry>Het geen begin-of eindpunte nie</entry>
              <entry/>
              <entry>vlak</entry>
            </row>
            <row>
              <entry>Het ’n beginpunt, maar gaan onbe­perk voort</entry>
              <entry/>
              <entry>punt</entry>
            </row>
            <row>
              <entry>Het lengte en breedte maar nie hoogte nie</entry>
              <entry/>
              <entry>reguit lyn</entry>
            </row>
          </tbody>
        </tgroup>
      </table>
      <para id="id8986731">(4)</para>
