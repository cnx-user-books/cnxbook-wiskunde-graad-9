        </para>
        <para id="id7156752">
          <m:math>
            <m:semantics>
              <m:mrow>
                <m:mstyle fontsize="12pt">
                  <m:mrow>
                    <m:mfrac>
                      <m:mrow>
                        <m:mn>4</m:mn>
                        <m:mo stretchy="false">+</m:mo>
                        <m:mi>x</m:mi>
                      </m:mrow>
                      <m:mrow>
                        <m:mi>x</m:mi>
