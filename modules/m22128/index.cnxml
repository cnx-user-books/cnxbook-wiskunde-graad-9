        <figure id="id2296581">
          <media id="id2296581_media" alt="">
            <image mime-type="image/png" src="../../media/graphics10-db46.png" id="id2296581__onlineimage" height="65" width="132"/>
          </media>
        </figure>
        <para id="id2176117">e)  </para>
        <figure id="id5214671">
          <media id="id5214671_media" alt="">
            <image mime-type="image/png" src="../../media/graphics11-20ed.png" id="id5214671__onlineimage" height="115" width="190"/>
          </media>
