        <para id="id5778112">f)</para>
        <figure id="id5999829">
          <media id="id5999829_media" alt="">
            <image mime-type="image/png" src="../../media/graphics12-d961.png" id="id5999829__onlineimage" height="100" width="100"/>
          </media>
